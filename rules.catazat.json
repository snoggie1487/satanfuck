{
	"$schema": "https://raw.githubusercontent.com/PazerOP/tf2_bot_detector/master/schemas/v3/rules.schema.json",
	"file_info": {
		"authors": [
			"Catazat"
		],
		"description": "meow",
		"title": "meow",
		"update_url": "https://raw.githubusercontent.com/moebkun/lists/main/rules.beta.json"
	},
	"rules": [
		{
			"actions": {
				"transient_mark": [
					"cheater"
				]
			},
			"description": "Invisible Name",
			"triggers": {
				"avatar_match": [
					{
						"avatar_hash": ""
					}
				],
				"mode": "match_any",
				"username_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"\u205f",
						"\u200f",
						"\u200e",
						"\u200d",
						"\u200c",
						"\u200b",
						"\u2060",
						"\u202c",
						"\u202d",
						"\ufeff",
						"\u0e4a",
						"\u0e38"
						"\n"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"racist"
				]
			},
			"description": "Bigoted Message",
			"triggers": {
				"avatar_match": [
					{
						"avatar_hash": ""
					}
				],
				"chatmsg_text_match": {
					"case_sensitive": false,
					"mode": "regex",
					"patterns": [
						"fag"
						"\\w*faggot\\w*"
						"nig"
					]
				},
				"mode": "match_any"
			}
		},
		{
			"actions": {
				"mark": [
					"racist"
				]
			},
			"description": "Bigoted Name",
			"triggers": {
				"avatar_match": [
					{
						"avatar_hash": ""
					}
				],
				"mode": "match_any",
				"username_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"nig",
						"faggot",
						"fag",
						"nigger"
					]
				}
			}
		}
	]
}
